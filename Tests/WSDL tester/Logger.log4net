    <from value="HlidacSmluv@vyvojar.cz" />
    <subject value="Error from HlidacSmluv worker" />
    <PickupDir value="c:\Inetpub\mailroot\Pickup" />


    <bufferSize value="0" />
    <lossy value="false" />


