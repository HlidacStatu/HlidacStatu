@using Nest
@using System
@using System.Collections.Generic
@using System.Linq
@using System.Web
@using System.Web.Mvc
@using HlidacStatu.Web.Framework
@using HlidacStatu.Lib;
@using HlidacStatu.Lib.Render;

@{

    var model = new HlidacStatu.Lib.Searching.SmlouvaSearchResult(this.Request.QueryString, HlidacStatu.Lib.Data.Smlouva.Search.OrderResult.DateAddedDesc);
    model.Q = "(issues.issueTypeId:18 OR issues.issueTypeId:12)";
    model = HlidacStatu.Lib.Data.Smlouva.Search.SimpleSearch(model.Q, model.Page,
100, (HlidacStatu.Lib.Data.Smlouva.Search.OrderResult)(Convert.ToInt32(model.Order)), platnyZaznam: true);

    //model.Chyby = true;
}


<p class="text-muted">Smlouvy skrývající smluvní strany s odkazem na obchodní tajemství</p>
<p><a href="/Report/11">Přehledný souhrn největších odběratelů skrývajících smluvní strany</a></p>

@Html.Partial("_SearchResult", model)


